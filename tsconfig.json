{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "Node",
    "outDir": "./build",
    "rootDir": "./src",
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "removeComments": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@Clients": ["src/Clients"],
      "@Commands/*": ["src/Commands/*"],
      "@Core/*": ["src/Core/*"],
      "@Embeds": ["src/Embeds"],
      "@Events": ["src/Events"],
      "@Options": ["src/Options"],
      "@Handlers": ["src/Handlers"]
    },
  },
  "include": [
    "./src"
  ]
}
